<h1> This is home page interviewer </h1>
<p id="notice"><%= notice %></p>

<h1>Requests</h1>

<table>
  <thead>
    <tr>
      <th>Date 1</th>
      <th>Time 1</th>
      <th>Date 2</th>
      <th>Time 2</th>
      <th>Date 3</th>
      <th>Time 3</th>
      <th>Date 4</th>
      <th>Time 4</th>
      <th>Date 5</th>
      <th>Time 5</th>
      <th>Mode</th>
      <th>Approved</th>
      <th>Approved date</th>
      <th>Approved time</th>
      <th>Candidate</th>
      <th>Interviewer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.date_1 %></td>
        <td><%= request.time_1 %></td>
        <td><%= request.date_2 %></td>
        <td><%= request.time_2 %></td>
        <td><%= request.date_3 %></td>
        <td><%= request.time_3 %></td>
        <td><%= request.date_4 %></td>
        <td><%= request.time_4 %></td>
        <td><%= request.date_5 %></td>
        <td><%= request.time_5 %></td>
        <td><%= request.mode %></td>
        <td><%= request.approved %></td>
        <td><%= request.approved_date %></td>
        <td><%= request.approved_time %></td>
        <td><%= request.candidate %></td>
        <td><%= request.interviewer %></td>
        <td><%= link_to "Approve", interviewer_dashboard_approve_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
