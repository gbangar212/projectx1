<p id="notice"><%= notice %></p>

<h1>Supports</h1>

<table>
  <thead>
    <tr>
      <th>Candidate</th>
      <th>Interviewer</th>
      <th>Request</th>
      <th>Type</th>
      <th>Message</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supports.each do |support| %>
      <tr>
        <td><%= support.candidate %></td>
        <td><%= support.interviewer %></td>
        <td><%= support.request %></td>
        <td><%= support.request_type %></td>
        <td><%= support.message %></td>
          <td><%= support.status %></td>
        <td><%= link_to 'Show', support %></td>
        <td><%= link_to 'Edit', edit_support_path(support) %></td>
        <td><%= link_to 'Destroy', support, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Support', new_support_path %>
